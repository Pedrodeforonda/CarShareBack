{
  "apps": [
    {
      "name": "carshare-backend",
      "script": "dist/app.js",
      "cwd": "/home/ubuntu/carshare-backend",
      "instances": 1,
      "autorestart": true,
      "watch": false,
      "max_memory_restart": "1G",
      "env": {
